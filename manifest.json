{
  "manifest_version": 2,
  "name": "Unlike follower",
  "description": "unlike follower client",
  "version": "0.0.1",
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "images/content/*"
  ]
}
